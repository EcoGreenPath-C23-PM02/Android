<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/light_green_background"
    tools:context=".ui.socialConnect.SocialConnectActivity">

    <ImageView
        android:id="@+id/underConstructIV"
        android:layout_width="220dp"
        android:layout_height="220dp"
        android:elevation="15dp"
        android:src="@drawable/phone_maintenance_pana"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.334" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/nunito_bold"
        android:text="Opps...."
        android:textColor="@color/font_color3"
        android:textSize="25sp"
        app:layout_constraintStart_toStartOf="@+id/underConstructIV"
        app:layout_constraintTop_toBottomOf="@+id/underConstructIV" />

    <TextView
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:text="This page still under development"
        android:fontFamily="@font/nunito_bold"
        android:textColor="@color/font_color3"
        android:textSize="20sp"
        android:gravity="center"
        app:layout_constraintEnd_toEndOf="@+id/underConstructIV"
        app:layout_constraintStart_toStartOf="@+id/underConstructIV"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

</androidx.constraintlayout.widget.ConstraintLayout>